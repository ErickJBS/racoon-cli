{
  "name": "racoon-cli",
  "version": "0.1.2",
  "description": "Express Aplication Project Generator",
  "bin": {
    "racoon": "dist/bin/racoon.js"
  },
  "scripts": {
    "copy-templates": "copyfiles -u 2 src/templates/**/* dist/templates/",
    "clean": "rimraf dist/",
    "lint": "eslint \"src/{bin,lib,commands,actions}/**/*.ts\" --fix",
    "prebuild": "npm run lint",
    "build": "npm run clean && npm run copy-templates && tsc",
    "dev": "npm run build && npm start",
    "start": "node dist/bin/racoon.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ErickJBS/racoon-cli.git"
  },
  "author": "Erick Blanco <erick.jassi.bs@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ErickJBS/racoon-cli/issues"
  },
  "homepage": "https://github.com/ErickJBS/racoon-cli#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "handlebars": "^4.7.6",
    "ora": "^4.1.0",
    "shelljs": "^0.8.4",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/copyfiles": "^2.2.0",
    "@types/handlebars": "^4.1.0",
    "@types/node": "^14.0.27",
    "@types/ora": "^3.2.0",
    "@types/shelljs": "^0.8.8",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "copyfiles": "^2.3.0",
    "eslint": "^7.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  }
}
